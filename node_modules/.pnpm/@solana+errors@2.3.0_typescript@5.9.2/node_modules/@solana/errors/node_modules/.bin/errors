#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/D05AC7DA5AC7BC0A/web3/node_modules/.pnpm/@solana+errors@2.3.0_typescript@5.9.2/node_modules/@solana/errors/bin/node_modules:/mnt/D05AC7DA5AC7BC0A/web3/node_modules/.pnpm/@solana+errors@2.3.0_typescript@5.9.2/node_modules/@solana/errors/node_modules:/mnt/D05AC7DA5AC7BC0A/web3/node_modules/.pnpm/@solana+errors@2.3.0_typescript@5.9.2/node_modules/@solana/node_modules:/mnt/D05AC7DA5AC7BC0A/web3/node_modules/.pnpm/@solana+errors@2.3.0_typescript@5.9.2/node_modules:/mnt/D05AC7DA5AC7BC0A/web3/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/D05AC7DA5AC7BC0A/web3/node_modules/.pnpm/@solana+errors@2.3.0_typescript@5.9.2/node_modules/@solana/errors/bin/node_modules:/mnt/D05AC7DA5AC7BC0A/web3/node_modules/.pnpm/@solana+errors@2.3.0_typescript@5.9.2/node_modules/@solana/errors/node_modules:/mnt/D05AC7DA5AC7BC0A/web3/node_modules/.pnpm/@solana+errors@2.3.0_typescript@5.9.2/node_modules/@solana/node_modules:/mnt/D05AC7DA5AC7BC0A/web3/node_modules/.pnpm/@solana+errors@2.3.0_typescript@5.9.2/node_modules:/mnt/D05AC7DA5AC7BC0A/web3/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/cli.mjs" "$@"
else
  exec node  "$basedir/../../bin/cli.mjs" "$@"
fi
